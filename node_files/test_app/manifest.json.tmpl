{
	"type" : "binary",
        "slave": "/usr/bin/cocaine-worker-generic",
	"engine" : {
		"heartbeat-timeout" : 60,
		"pool-limit" : 10,
		"queue-limit" : 2000,
		"grow-threshold" : 1,
		"slave" : "/tmp/build/bin/cocaine-slave"
	},
	"args" : {
		"name" : "<{node_config.soname}>",
		"config" : {
	                "nodes" : {
				"<{node}>": 1025
                	},
	                "groups" : [1, 2],
			"log-level" : 3,
			"log" : "/dev/stdout"
		}
	}
}
